{
  "extends": ["eslint:recommended", "plugin:react/recommended", "plugin:prettier/recommended"],
  "plugins": ["react", "react-hooks"],
  "rules": {
    "prettier/prettier": "warn",
    "react-hooks/exhaustive-deps": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 2018
  }
}
